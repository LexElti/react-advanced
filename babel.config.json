{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    [
      // preset-react потребовался для RTL для теста React-компонента
      "@babel/preset-react",
      {
        // чтобы в тестах не импортироать React
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "i18next-extract"
  ]
}
